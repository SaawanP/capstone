{
  "ShowLogs": false,
  "LiveCompileQss": false,
  "CheckForMissingicons": false,
	"QMainWindow": [
	  {
		"tittle": "FYDP Final GUI",
		"icon": "Qss/Icons/font_awesome/solid/robot.png",
		"frameless": true,
		"transluscentBg": true,
		"sizeGrip": "sizeGrip",
		"navigation": [
		  {
			"minimize": "minimizeBtn",
			"close": "closeBtn",
			"expand": "expandBtn",
			"restore": [
			  {
				"buttonName": "restoreBtn",
				"normalIcon": "Qss/Icons/feather/square.png",
				"maximizedIcon": "Qss/Icons/feather/square.png"
			  }
			],
			"moveWindow": "headerContainer",
			"tittleBar": "headerContainer"
		  }
		]
	  }
	],
  "QCustomSlideMenu": [
	  {
		"name": "leftMenuContainer",
		"defaultSize": [
		  {
			"width": 40,
			"height": "parent"
		  }
		],
		"collapsedSize": [
		  {
			"width": 40,
			"height": "parent"
		  }
		],
		"expandedSize": [
		  {
			"width": 130,
			"height": "parent"
		  }
		],
		"toggleButton": [
		  {
			"buttonName": "menuBtn",
			"icons": [
			  {
				"whenMenuIsCollapsed": "Qss/Icons/feather/align-justify.png",
				"whenMenuIsExpanded": "Qss/Icons/feather/arrow-left-circle.png"
			  }
			]
		  }
		],
		"menuTransitionAnimation": [
		  {
			"animationDuration": 500,
			"animationEasingCurve": "Linear"
		  }
		]
	  },
	  {
		"name": "rightMenuContainer",
		"defaultSize": [
		  {
			"width": 0,
			"height": "parent"
		  }
		],
		"collapsedSize": [
		  {
			"width": 0,
			"height": "parent"
		  }
		],
		"expandedSize": [
		  {
			"width": 200,
			"height": "parent"
		  }
		],
		"menuTransitionAnimation": [
		  {
			"animationDuration": 500,
			"animationEasingCurve": "Linear"
		  }
		]
	  }
	],
  "QCustomQStackedWidget": [
	  {
		"name": "mainPages",
		"navigation": [
		  {
			"navigationButtons": [
			  {
				"homeBtn": "home_page",
				"dataBtn": "data_page",
				"reportBtn": "reports_page"
			  }
			]
		  }
		],
		"transitionAnimation": [
		  {
			"fade": [
			  {
				"active": true,
				"duration": 500,
				"easingCurve": "InSine"
			  }
			]
		  }
		]
	  },
	  {
		"name": "centerMenuPages",
		"navigation": [
		  {
			"navigationButtons": [
			  {
				"settingsBtn": "settings_page",
				"infoBtn": "info_page",
				"helpBtn": "help_page"
			  }
			]
		  }
		]
	  },
	  {
		"name": "rightMenuPages",
		"navigation": [
		  {
			"navigationButtons": [
			  {
				"notificationBtn": "notification_page",
				"moreMenuBtn": "more_page",
				"profileMenuBtn": "profile_page",
				"settingsBtn": "settings_page",
				"infoBtn": "info_page",
				"helpBtn": "help_page"
			  }
			]
		  }
		]
	  }
	],
  "QPushButtonGroup": [
	  {
		"Buttons": [
		  "homeBtn",
		  "dataBtn",
		  "reportBtn"
		],
		"ActiveButton": "homeBtn",
		"Style": [
		  {
			"Active": "QPushButton{background-color: THEME.COLOR_BACKGROUND_2; font-weight: bold; border-top: 2px solid $COLOR_BACKGROUND_3; border-left: 5px solid $COLOR_BACKGROUND_3;border-bottom: 6px solid $COLOR_BACKGROUND_3;}",
			"NotActive": "QPushButton{background-color: transparent; font-weight: normal; border-top: 2px solid transparent; border-left: 5px solid transparent;border-bottom: 6px solid transparent;}"
		  }
		]
	  },
	  {
		"Buttons": [
		  "settingsBtn",
		  "infoBtn",
		  "helpBtn"
		],
		"Style": [
		  {
			"Active": "QPushButton{background-color: THEME.COLOR_ACCENT_1; font-weight: bold; border-top: 6px solid $COLOR_BACKGROUND_3; border-left: 6px solid $COLOR_BACKGROUND_3;border-bottom: 6px solid $COLOR_BACKGROUND_3;}",
			"NotActive": "QPushButton{background-color: transparent; font-weight: normal; border-top: 6px solid transparent; border-left: 6px solid transparent;border-bottom: 6px solid transparent;}"
		  }
		]
	  },
	  {
		"Buttons": [
		  "notificationBtn",
		  "moreMenuBtn",
		  "profileMenuBtn",
		  "closeRightMenuBtn"
		],
		"Style": [
		  {
			"Active": "QPushButton{background-color: THEME.COLOR_BACKGROUND_2; font-weight: bold; border-radius: 5px} QPushButton::hover{background-color: THEME.COLOR_ACCENT_1}",
			"NotActive": "QPushButton{background-color: transparent; font-weight: normal; border-radius: 5px} QPushButton::hover{background-color: THEME.COLOR_ACCENT_1}"
		  }
		]
	  }
	],
	"QSettings": [
	  {
		"ThemeSettings": [
		  {
			"CustomTheme": [
			  {
				"Background-color": "#FFFFFF",
				"Text-color": "#010000",
				"Icons-color": "#000000",
				"Accent-color": "#26bae3",
				"Theme-name": "LightBlue",
				"Default-Theme": true
			  },
			  {
				"Background-color": "#21272a",
				"Text-color": "#fefefe",
				"Icons-color": "#fefefe",
				"Accent-color": "#fba43b",
				"Theme-name": "DarkYellow",
				"Default-Theme": false
			  }
			]
		  }
		],
      "AppSettings": {
        "OrginizationName": "first",
        "ApplicationName": "first Company",
        "OrginizationDormain": "first.org"
      }
    }
  ],
  "QtBinding": "PySide6"
}
